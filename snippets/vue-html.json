{
  "a-modal": {
    "prefix": "a-modal",
    "body": [
      "<a-modal",
      "  :maskClosable=\"false\"",
      "  :destroyOnClose=\"true\"",
      "  title=\"修改\"",
      "  v-model=\"visible\"",
      "  :width=\"600\"",
      "  @ok=\"onSubmit\"",
      ">",
      "$1",
      "</a-modal>"
    ]
  },
  "a-form(vue3-antd-design)": {
    "prefix": "a-form",
    "body": [
      "<a-form",
      "  ref=\"form\"",
      "  :model=\"form\"",
      "  :label-col=\"{ span: 4 }\"",
      "  :wrapper-col=\"{ span: 14 }\"",
      "  :rules=\"rules\"",
      "  @submit.prevent=\"onSubmit\"",
      "  >",
      "  <a-form-item label=\"标题\" name=\"title\">",
      "    <a-input",
      "      placeholder=\"请输入\"",
      "      v-model=\"form.title\"",
      "    ></a-input>",
      "  </a-form-item>",
      "  <a-form-item :wrapper-col=\"{ span: 14, offset: 4 }\">",
      "    <a-button size=\"large\" htmlType=\"submit\" type=\"primary\">确认保存</a-button>",
      "  </a-form-item>",
      "</a-form>"
    ]
  },
  "a-form-model(vue2-antd-design)": {
    "prefix": "a-form-model",
    "body": [
      "<a-form-model",
      "  ref=\"form\"",
      "  :model=\"form\"",
      "  :label-col=\"{ span: 4 }\"",
      "  :wrapper-col=\"{ span: 14 }\"",
      "  :rules=\"rules\"",
      "  @submit.prevent=\"onSubmit\"",
      "  >",
      "  <a-form-model-item label=\"标题\" prop=\"title\">",
      "    <a-input",
      "      placeholder=\"请输入\"",
      "      v-model=\"form.title\"",
      "    ></a-input>",
      "  </a-form-model-item>",
      "  <a-form-model-item :wrapper-col=\"{ span: 14, offset: 4 }\">",
      "    <a-button size=\"large\" htmlType=\"submit\" type=\"primary\">确认保存</a-button>",
      "  </a-form-model-item>",
      "</a-form-model>"
    ]
  }
}
